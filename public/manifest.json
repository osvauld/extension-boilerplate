{
    "manifest_version": 2,
    "name": "My Svelte Chrome Extension",
    "version": "1.0",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ],
    "browser_action": {
        "default_popup": "index.html"
    }
}